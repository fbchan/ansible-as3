{
  "class": "AS3",
  "action": "deploy",
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.11.0",
    "id": "demo3",
    "label": "demo3",
    "remark": "Declaration for UDP Load Balancer Service",
    "demo3": {
      "class": "Tenant",
      "demo3_udp_apps": {
        "class": "Application",
        "template": "udp",
        "serviceMain": {
          "class": "Service_UDP",
          "virtualPort": 514,
          "virtualAddresses": [
            "1.1.1.13"
          ],
          "pool": "demo3_udp_pool"
        },
        "demo3_udp_pool": {
          "class": "Pool",
          "monitors": [
            "icmp"
          ],
          "members": [
            {
              "servicePort": 514,
                                "shareNodes": true,
                            "serverAddresses": [
                    "3.3.3.3",
                    "3.3.3.4",
                
              ]
            }
          ]
        }
      }
    }
  }
 }